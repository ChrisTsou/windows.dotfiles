{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "arch",
    "disabledProfileSources": 
    [
        "Windows.Terminal.Wsl",
        "Windows.Terminal.Azure",
        "Windows.Terminal.PowershellCore"
    ],
    "launchMode": "maximized",
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "schemes": [],
    "themes": [],
    "profiles": 
    {
        "defaults": 
        {
            "closeOnExit": "always",
            "font": 
            {
                "face": "FiraCode Nerd Font"
            }
        },
        "list": 
        [
            {
                "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
                "icon": "ms-appdata:///roaming/powershell.ico",
                "name": "PowerShell"
            },
            {
                "commandline": "wsl.exe -d arch",
                "icon": "ms-appdata:///roaming/archlinux.ico",
                "name": "Arch"
            }
        ]
    },
    "actions": 
    [
        {
            "command": 
            {
                "action": "sendInput",
                "input": "\u001b[13;2u"
            },
            "id": "User.shift_enter"
        },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "\u001b[13;5u"
            },
            "id": "User.ctrl_enter"
        },
        {
            "command": 
            {
                "action": "closeTab"
            },
            "id": "User.CloseTab"
        },
        {
            "command": 
            {
                "action": "commandPalette"
            },
            "id": "User.palette"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "previousInOrder"
            },
            "id": "User.MoveFocusPreviousInOrder"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "nextInOrder"
            },
            "id": "User.MoveFocusNextInOrder"
        },
        {
            "command": "closeWindow",
            "id": "User.closeWindow"
        }
    ],
    "keybindings": 
    [
        {
            "id": "Terminal.FindText",
            "keys": "ctrl+shift+/"
        },
        {
            "id": "User.CloseTab",
            "keys": "ctrl+shift+d"
        },
        {
            "id": "Terminal.ClosePane",
            "keys": "ctrl+shift+c"
        },
        {
            "id": "Terminal.ScrollDown",
